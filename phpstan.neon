includes:
    - ../../vendor/craftcms/phpstan/phpstan.neon

parameters:
    level: 5
    phpVersion: 80300
    paths:
        - src
    ignoreErrors:
        # Optional dependencies for third-party search services (may not be installed)
        # These patterns are marked reportUnmatched: false individually since packages are optional
        -
            identifier: class.notFound
            message: '#Algolia\\\\AlgoliaSearch#'
            reportUnmatched: false
        -
            identifier: class.notFound
            message: '#Meilisearch\\\\#'
            reportUnmatched: false
        -
            identifier: class.notFound
            message: '#Typesense\\\\#'
            reportUnmatched: false
